<mujoco>
    <extension>
        <plugin plugin="mujoco.elasticity.shell"/>
    </extension>
    <asset>
        <texture type="skybox" builtin="gradient" rgb1=".3 .5 .7" rgb2="0 0 0" width="32" height="512"/>
        <mesh file="../mesh/body/convex_0.stl" name="convex0" />
        <mesh file="../mesh/body/convex_1.stl" name="convex1" />
        <mesh file="../mesh/body/convex_2.stl" name="convex2" />
        <mesh file="../mesh/body/convex_3.stl" name="convex3" />
        <mesh file="../mesh/body/convex_4.stl" name="convex4" />
        <mesh file="../mesh/body/convex_5.stl" name="convex5" />
        <mesh file="../mesh/body/convex_6.stl" name="convex6" />
        <mesh file="../mesh/body/convex_7.stl" name="convex7" />
        <mesh file="../mesh/body/convex_8.stl" name="convex8" />
        <mesh file="../mesh/body/convex_9.stl" name="convex9" />
        <mesh file="../mesh/body/convex_10.stl" name="convex10" />
        <mesh file="../mesh/body/convex_11.stl" name="convex11" />
        <material name="mat_human" rgba="0.8 0.8 0.8 1"/>
    </asset>
    <worldbody>
        <light name="sunlight" pos="0 0 10" dir="0 0 -1" diffuse="1 1 0.9" specular="0.1 0.1 0.1" directional="true" castshadow="true"/>
        <geom name="floor" type="plane" size="0 0 .1"/>
        <body name="mesh_human" pos="0 0 0" euler="90 0 0">
            <geom type="mesh" mesh="convex0" material="mat_human"/>
            <geom type="mesh" mesh="convex1" material="mat_human"/>
            <geom type="mesh" mesh="convex2" material="mat_human"/>
            <geom type="mesh" mesh="convex3" material="mat_human"/>
            <geom type="mesh" mesh="convex4" material="mat_human"/>
            <geom type="mesh" mesh="convex5" material="mat_human"/>
            <geom type="mesh" mesh="convex6" material="mat_human"/>
            <geom type="mesh" mesh="convex7" material="mat_human"/>
            <geom type="mesh" mesh="convex8" material="mat_human"/>
            <geom type="mesh" mesh="convex9" material="mat_human"/>
            <geom type="mesh" mesh="convex10" material="mat_human"/>
            <geom type="mesh" mesh="convex11" material="mat_human"/>
        </body>
        <body pos="1.5 0.0 25" euler="0 0 45">
            <composite type="particle" count="9 19 1" spacing="0.5" flatinertia="0.01">
                <skin texcoord="true" inflate="0.005" subgrid="4"/>
                <geom type="ellipsoid" size="0.25 0.1 0.25" rgba=".8 .2 .1 1" group="4"/>
                <plugin plugin="mujoco.elasticity.shell">
                <config key="poisson" value="0"/>
                <config key="thickness" value="1e-2"/>
                <!--Units are in Pa (SI)-->
                <config key="young" value="3e6"/>
                </plugin>
            </composite>
        </body>
    </worldbody>
</mujoco>
 